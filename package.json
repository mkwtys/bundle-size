{
  "name": "bundle-size",
  "description": "Show bundle file size of npm packages",
  "version": "1.1.5",
  "author": "mkwtys <mkwtys@gmail.com>",
  "bin": {
    "bundle-size": "bin/bundle-size.js"
  },
  "bugs": {
    "url": "https://github.com/mkwtys/bundle-size/issues"
  },
  "dependencies": {
    "browserify": "17.0.0",
    "cli-table3": "0.6.4",
    "envify": "4.1.0",
    "minimist": "1.2.8",
    "ora": "8.0.1",
    "pretty-bytes": "6.1.1",
    "uglify-js": "3.17.4"
  },
  "devDependencies": {
    "jest": "29.7.0"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "bin",
    "lib"
  ],
  "homepage": "https://github.com/mkwtys/bundle-size",
  "keywords": [
    "bundle",
    "envify",
    "gzip",
    "size",
    "uglify"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mkwtys/bundle-size.git"
  },
  "scripts": {
    "coverage": "jest --coverage",
    "test": "jest"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "fixtures"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "fixtures"
    ]
  }
